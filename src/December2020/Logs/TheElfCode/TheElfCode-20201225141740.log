      elf.moveDown(direction + j);
    if (direction == 2)
      elf.moveLeft(direction + j);
    if (direction == 3)
      elf.moveUp(direction + j);
    if (direction == 4)
      elf.moveRight(direction + j);
    elf.pull_lever(direction - 1 + j);
  }
  
