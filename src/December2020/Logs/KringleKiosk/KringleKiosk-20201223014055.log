                echo
                echo "Escape the menu by launching /bin/bash"
                echo
                echo
                read -n 1 -r -s -p $'Press enter to continue...'
                clear

                # -----------------------------------
                # Step #5: Main logic - infinite loop
                # ------------------------------------

                while true; do
                  show_menus
                  read_options
                done



            
